{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "no-undef": "off",
      "no-underscore-dangle": [
        "error", {
          "allowAfterThis": true,
          "allow": ["_exports", "_directories"]
        }]
    }
}
